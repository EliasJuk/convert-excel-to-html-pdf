<!DOCTYPE html>
<html>
  <head>
    <title>Table</title>
  </head>
  <body>

    <table border="1">
      <thead>
        <% header.forEach(col => { %>
          <th> <%= col %> </th>
        <% }) %> 
      </thead>
      <tbody>
        <% rows.forEach(row => { %>
          <tr> 
            <% row.forEach(element => { %>
              <td><%= element %></td>
            <%  }) %>
          </tr>
        <% }) %>
      </tbody>
    </table>

  </body>
</html>